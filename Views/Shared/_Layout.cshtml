<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TP_Final_4IC</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TP_Final_4IC.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar">
    <div class="container">
      <a class="navbar-brand" href="#">Navbar</a>
      <div class="navbar-nav">
        <a class="nav-item" href="#">Inicio</a>
        <a class="nav-item" href="#">Productos</a>
        <a class="nav-item" href="#">Contacto</a>
      </div>
      <div class="navbar-user">
        <a class="nav-link" href="#">Usuario</a>
        <input type="text" class="form-control" placeholder="Buscar">
      </div>
    </div>
  </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
